{
  "extends": ["@commitlint/config-conventional"],
  "rules": {
    "type-enum": [
      2,
      "always",
      ["feat", "fix", "docs", "style", "refactor", "perf", "test", "build", "ci", "chore", "revert"]
    ],
    "subject-case": [0],
    "scope-enum": [
      1,
      "always",
      ["git-sync", "metadata-manager", "publisher-scripton", "shared", "monorepo", "deps"]
    ]
  }
}
