{
  "name": "@sb-obsidian-plugins/template-generator",
  "version": "1.0.0",
  "description": "Obsidian plugin for generating templates with dynamic content",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "lint": "eslint src --ext .ts",
    "test": "vitest",
    "clean": "rm -rf dist lib",
    "release": "npm run build && zip -r template-generator.zip main.js manifest.json styles.css"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "template",
    "generator"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@sb-obsidian-plugins/shared": "workspace:*",
    "@types/node": "^24.10.1",
    "builtin-modules": "^5.0.0",
    "esbuild": "^0.27.0",
    "obsidian": "latest",
    "tslib": "^2.8.1"
  },
  "dependencies": {
    "handlebars": "^4.7.8",
    "moment": "^2.30.1"
  }
}
