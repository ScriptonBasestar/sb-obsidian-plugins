{
  "name": "@sb-obsidian-plugins/blog-platform-export",
  "version": "0.1.0",
  "description": "Export Obsidian notes to blog platforms (Hugo, Jekyll, WikiJS)",
  "main": "src/main.ts",
  "scripts": {
    "build": "esbuild src/main.ts --bundle --outfile=main.js --external:obsidian --format=cjs --target=es2018",
    "dev": "npm run build -- --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@sb-obsidian-plugins/shared": "workspace:*",
    "@sb-obsidian-plugins/wikijs-sync": "workspace:*",
    "unified": "^10.1.2",
    "remark": "^14.0.3", 
    "remark-parse": "^10.0.2",
    "remark-stringify": "^10.0.3",
    "remark-frontmatter": "^4.0.1",
    "remark-wiki-link": "^1.0.4",
    "js-yaml": "^4.1.0",
    "toml": "^3.0.0",
    "fs-extra": "^11.1.1",
    "glob": "^8.1.0",
    "handlebars": "^4.7.7",
    "moment": "^2.29.4"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
    "@types/js-yaml": "^4.0.5",
    "esbuild": "^0.19.2",
    "obsidian": "latest",
    "typescript": "^5.0.4",
    "vitest": "^0.34.0"
  },
  "keywords": ["obsidian", "plugin", "blog", "export", "hugo", "jekyll", "wikijs"],
  "author": "SB Obsidian Plugins",
  "license": "MIT"
}